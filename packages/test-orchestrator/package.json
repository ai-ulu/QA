{
  "name": "@autoqa/test-orchestrator",
  "version": "1.0.0",
  "description": "Test execution orchestrator for AutoQA Pilot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@autoqa/cache": "workspace:*",
    "@autoqa/database": "workspace:*",
    "bull": "^4.12.2",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "fast-check": "^4.5.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "keywords": [
    "autoqa",
    "test-orchestrator",
    "kubernetes",
    "docker",
    "playwright"
  ],
  "author": "AutoQA Team",
  "license": "MIT"
}
