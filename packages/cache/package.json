{
  "name": "@autoqa/cache",
  "version": "1.0.0",
  "description": "Redis-based caching and rate limiting for AutoQA Pilot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:property": "jest --selectProjects property",
    "test:integration": "jest --selectProjects integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.7",
    "fast-check": "^3.15.0",
    "@types/jest": "^29.5.8"
  }
}